{
    "New todo": {
		"prefix": "new",
		"body": [
			"* [ - ] $1"
		],
		"description": "A new line for todo"
	},
    "Done todo": {
		"prefix": "done",
		"body": [
			"* [ + ] $1"
		],
		"description": "A new line for an already ready task"
	},
    "Url": {
		"prefix": "url-from-selection",
		"body": [
			"[$TM_SELECTED_TEXT]($CLIPBOARD)"
		],
		"description": "Add a basic link from the selected text"
	},
    "Read urll": {
		"prefix": "read",
		"body": [
			"* [ - ] [$1]($CLIPBOARD)"
		],
		"description": "Add a todo for a link"
	},
    "source": {
		"prefix": "source",
		"body": [
			"[source]($CLIPBOARD)"
		],
		"description": "Add source"
	}
}
